[codespell]
# Ref: https://github.com/codespell-project/codespell#using-a-config-file
skip = .git*,vendor,*-lock.yaml,*.lock,.codespellrc,.codespellignore,*test.ts,*.jsonl,frame*.txt,*.snap,*.snap.new,*.map,**/*.map,**/dist*,**/target*,codex-rs/target*,node_modules*,**/node_modules*
check-hidden = true
ignore-words = .codespellignore
ignore-regex = ^\s*"image/\S+": ".*|\b(afterAll)\b
ignore-words-list = ratatui,ser,iTerm,iterm2,iterm,ANE
